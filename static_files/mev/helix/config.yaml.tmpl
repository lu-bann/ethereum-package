postgres:
  hostname: {{ .Hostname }}
  port: 5432
  db_name: {{ .DbName }}
  user: {{ .User }}
  password: {{ .Password }}
  ssl_mode: {{ .SslMode }}
  region: {{ .Region }}
  region_name: {{ .RegionName }}
redis:
  url: "redis://127.0.0.1:6379"
broadcasters:
  - !BeaconClient
      url: "http://127.0.0.1:4000"
simulator:
  url: "http://127.0.0.1:8545"
beacon_clients:
  - !BeaconClientConfig:
      url: "http://127.0.0.1:4000"
relays: []
network_config: !Custom 
    dir_path: {{ .dir_path }}
    genesis_validator_root: {{ .genesis_validator_root }}
    genesis_time: {{ .genesis_time }}
logging: !Console
validator_preferences:
  filtering: "regional"
  trusted_builders: null
  header_delay: true
router_config: 
  enabled_routes:
    - route: !All
      rate_limit: null
target_get_payload_propagation_duration_ms: 1000